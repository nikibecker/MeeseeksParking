<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="16dp"
    android:paddingRight="16dp" >

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="goToOverview"
        android:text="back"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button7"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/button7"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Parking Overview"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button7"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Parking Lots"
        android:textAlignment="center" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText5"
        android:layout_toStartOf="@+id/lot_availability"
        android:onClick="goToActivityLot5"
        android:text="Lot 5" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button8"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button8"
        android:onClick="goToActivityLot7"
        android:text="Lot 7" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/structure_numbers"
        android:layout_alignParentStart="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Parking Structures"
        android:textAlignment="center" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button9"
        android:layout_marginTop="97dp"
        android:layout_toStartOf="@+id/structure_availability"
        android:onClick="goToActivityPS1"
        android:text="Structure 1" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button12"
        android:layout_toStartOf="@+id/structure_numbers2"
        android:onClick="goToActivityPS3"
        android:text="Structure 3" />

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button10"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button10"
        android:onClick="goToActivityPS2"
        android:text="Structure 2" />

    <EditText
        android:id="@+id/lot_availability"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/editText5"
        android:layout_alignBottom="@+id/editText5"
        android:layout_toEndOf="@+id/editText5"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Availability"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/lot_numbers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/editText5"
        android:layout_toEndOf="@+id/editText5"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Numbers"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/lot_numbers3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button8"
        android:layout_toEndOf="@+id/editText6"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Numbers"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/structure_availability"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/editText6"
        android:layout_alignBottom="@+id/editText6"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/editText6"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Availability"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/structure_numbers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button10"
        android:layout_alignBottom="@+id/button10"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/editText6"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Numbers"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/structure_numbers2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/structure_numbers3"
        android:layout_toEndOf="@+id/button12"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Numbers"
        android:textAlignment="center" />

    <EditText
        android:id="@+id/structure_numbers3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button11"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/button12"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Numbers"
        android:textAlignment="center" />

</RelativeLayout>